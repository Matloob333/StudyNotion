{
  "name": "studynotion",
  "version": "1.0.0",
  "description": "A comprehensive study management platform built with MERN stack",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "build-client": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build-client",
    "render-postbuild": "npm run install-client && npm run build-client",
    "postinstall": "npm run install-client && npm run build-client",
    "seed": "node scripts/seed-courses.js",
    "add-videos": "node scripts/add-videos.js"
  },
  "keywords": [
    "mern",
    "study",
    "education",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "StudyNotion Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.0.1",
    "google-auth-library": "^10.1.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
